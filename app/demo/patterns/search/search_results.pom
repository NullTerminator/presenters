require_relative 'helpers/search'

Voom::Presenters.define(:search_results) do
  content id: :search_results do
    grid do
      column 12 do
        display 'Search Results'
        subheading "#{Time.now}"
        subheading ' '
        list do
          context.map {|k, v| "#{k}: #{v}"}.each do |param|
            line param
          end
        end
      end
    end
  end
end
