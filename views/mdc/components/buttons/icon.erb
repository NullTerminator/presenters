  <button id="<%=comp.id%>"
        class="mdl-button mdl-js-button mdl-button--icon
         <%= 'mdl-button--colored' if comp.color==:primary %>
         <%= 'mdl-button--accent' if comp.color==:secondary %>
         <%= "vml-js-dialog-button" if comp.dialog %>"
         <%= "data-dialog-id='#{comp.dialog}'" if comp.dialog %>"
         <%= 'disabled' if comp.disabled %>
        <%= erb :"components/event", :locals => {event: comp.event, parent_id: comp.id} unless comp.disabled %>>
    <%= erb :"components/icon", :locals => {comp: comp, class_name: nil} %>
    <%= comp.text %>
</button>

