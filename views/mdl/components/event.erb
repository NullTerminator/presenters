<% if event %>
  on<%= event.event %>="new VEvents([
  <%
    event.actions.each do |action|
      additional_params = action.action_type.to_s=='replaces' ? "{grid_nesting:#{@grid_nesting-1}}" : '{}'
  %>
      ['<%=action.action_type%>','<%=action.url%>','<%=action.target%>',<%=additional_params%>],
  <% end %>
  ]).call()"
<% end %>